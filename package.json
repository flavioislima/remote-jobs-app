{
	"name": "remotework",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"rebuild": "rm -Rf node_modules & rm package-lock.json & yarn",
		"test": "jest",
		"eject": "rm -Rf ios android & react-native eject",
		"try-release": "react-native run-android --variant=release",
		"release": "cd ./android && ./gradlew assembleRelease",
		"clean": "cd ./android && ./gradlew clean build"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"moment": "^2.22.2",
		"react": "16.3.0",
		"react-native": "0.54.0",
		"react-native-admob": "^2.0.0-beta.5",
		"react-native-elements": "^0.19.1",
		"react-native-rate": "^1.1.2",
		"react-native-vector-icons": "^5.0.0",
		"react-navigation": "^2.16.0"
	},
	"devDependencies": {
		"babel-jest": "23.6.0",
		"babel-preset-react-native": "4.0.1",
		"jest": "23.6.0",
		"react-test-renderer": "16.0.0"
	},
	"jest": {
		"preset": "react-native"
	}
}